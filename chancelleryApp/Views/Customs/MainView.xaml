<UserControl x:Class="chancelleryApp.Views.Customs.MainView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:chancelleryApp.Views.Customs"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Command="{Binding  ProductViewCommand}" Background="DarkViolet" Foreground="White" Margin="10">Продукция</Button>
            <Button Command="{Binding  ItemViewCommand}" Margin="10" Background="DarkViolet" Foreground="White">Товары</Button>
            <Button Command="{Binding UserViewCommand }" Margin="10" Background="DarkViolet" Foreground="White">Пользователи</Button>
        </StackPanel>
    </Grid>
</UserControl>
